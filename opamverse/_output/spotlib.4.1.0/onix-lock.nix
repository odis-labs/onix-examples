    opam = "${repo}/packages/ppx_derivers/ppx_derivers.1.2.1/opam";
    depends = with self; [ dune ocaml ];
    buildDepends = with self; [ dune ocaml ];
  };
  ppx_test = {
    name = "ppx_test";
    version = "1.8.0";
    src = pkgs.fetchurl {
