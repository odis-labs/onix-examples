  };
  seq = {
    name = "seq";
    version = "base";
    opam = "${repo}/packages/seq/seq.base/opam";
    depends = with self; [ ocaml ];
    buildDepends = with self; [ ocaml ];
  };
  touist = {
    name = "touist";
    version = "3.5.0";
    src = pkgs.fetchurl {
      url = "https://github.com/touist/touist/archive/v3.5.0.tar.gz";
      sha256 = "1i20dps5npg3ncvlmyacscf58qzc0a6161m62n3d6vnjrql496l5";
    };
    opam = "${repo}/packages/touist/touist.3.5.0/opam";
    depends = with self; [ cmdliner jbuilder menhir minisat ocaml re
                           (self.qbf or null) (self.yices2 or null) ];
    buildDepends = with self; [ ocaml ];
  };
}
