    depends = with self; [ ocaml ocamlbuild ];
    buildDepends = with self; [ ocaml ocamlbuild ocamlfind ];
  };
  uring = {
    name = "uring";
    version = "0.3";
    src = pkgs.fetchurl {
      url = "https://github.com/ocaml-multicore/ocaml-uring/releases/download/v0.3/uring-0.3.tbz";
      sha256 = "47c225fce95ad34dd38c35f30724f7d8111afd4370d301985abc720747c6d746";
    };
    opam = "${repo}/packages/uring/uring.0.3/opam";
    depends = with self; [ cstruct dune dune-configurator fmt ocaml optint ];
    buildDepends = with self; [ dune ocaml ];
    depexts = with pkgs; [ (pkgs.linux-headers or null) ];
  };
}
